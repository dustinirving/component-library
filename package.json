{
  "name": "@dustinirving/component-library",
  "version": "1.0.0",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "types": "build/index.d.ts",
  "description": "React component library using Rollup, TypeScript, Styled-Components and Storybook",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "test": "npm run lint && jest --runInBand",
    "test:watch": "jest --watch",
    "lint": "npm run lint:types",
    "lint:types": "tsc --noEmit",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run build",
    "deploy": "build-storybook && gh-pages -d storybook-static && rm -r storybook-static",
    "prepare": "husky install && npm run build",
    "pretty-quick": "pretty-quick",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinirving/component-library.git"
  },
  "keywords": [
    "React",
    "Component",
    "Library",
    "Rollup",
    "Typescript",
    "Storybook"
  ],
  "author": "Dustin Irving",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dustinirving/component-library/issues"
  },
  "homepage": "https://github.com/dustinirving/component-library#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.8",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@storybook/addon-controls": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.5",
    "@storybook/addon-links": "^6.1.5",
    "@storybook/react": "^6.1.5",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^11.2.2",
    "@types/chai": "^4.3.0",
    "@types/chai-dom": "^0.0.12",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.0",
    "@types/react-datepicker": "^4.3.4",
    "@types/react-dom": "^17.0.0",
    "@types/react-select": "^3.1.2",
    "@types/styled-components": "^5.1.20",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^10.0.0",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.2",
    "chai-jest": "^1.0.2",
    "chai-style": "^1.0.3",
    "gh-pages": "^3.1.0",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "postcss": "^8.3.9",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rollup": "^2.33.3",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "semantic-release": "^17.4.7",
    "storybook-addon-designs": "^5.4.2",
